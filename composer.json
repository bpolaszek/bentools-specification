{
    "name": "bentools/specification",
    "type": "library",
    "description": "Provides an implementation of the Specification Pattern. PHP7+",
    "license": "MIT",
    "autoload-dev": {
        "psr-4": {
            "BenTools\\Specification\\Tests\\": "tests/src"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "BenTools\\Specification\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "@stable",
        "satooshi/php-coveralls": "@stable",
        "symfony/var-dumper": "^3.3"
    }
}
